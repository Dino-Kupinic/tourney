{
  "name": "tourney",
  "version": "0.0.1",
  "description": "This project is a web application for managing tournaments.",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build:tourney-admin": "pnpm --filter tourney-admin run build",
    "build:tourney-viewer": "pnpm --filter tourney-viewer run build",
    "build": "pnpm run build:tourney-admin && pnpm run build:tourney-viewer",
    "dev:admin": "nuxt dev --filter=tourney-admin",
    "dev:viewer": "nuxt dev --filter=tourney-viewer",
    "dev:core": "nuxt dev --filter=tourney-core"
  },
  "keywords": [
    "tournaments",
    "nuxt",
    "vue"
  ],
  "author": "Dino Kupinic",
  "license": "MIT",
  "dependencies": {
    "nuxt": "^3.13.1"
  }
}
